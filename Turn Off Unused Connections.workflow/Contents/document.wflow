				<key>ActionName</key>
				<string>Run Shell Script</string>
				<key>ActionParameters</key>
				<dict>
					<key>COMMAND_STRING</key>
					<string>#!/bin/bash

lastUsed=$(system_profiler SPAirPortDataType | grep "Last connected" | sed 's/Last connected: //')
lastUsedTimestamp=$(date -jf "%a %b %d %T %Y" "$lastUsed" +"%s")
currentTime=$(date +"%s")
difference=$((currentTime - lastUsedTimestamp))
maxTime=$((2 * 60 * 60)) # 2 hours in seconds

if [ $difference -gt $maxTime ]; then
    networksetup -setairportpower en0 off  # Replace en0 with your Wi-Fi interface if it's different
fi
</string>
