				<dict>
					<key>COMMAND_STRING</key>
					<string>#!/bin/bash

lastUsed=$(system_profiler SPBluetoothDataType | awk '/Last Connected:/ {print $3, $4, $5, $6, $7}')
lastUsedTimestamp=$(date -jf "%b %d %T %Y" "$lastUsed" +"%s" 2&gt;/dev/null || date -jf "%b %e %T %Y" "$lastUsed" +"%s" 2&gt;/dev/null)
currentTime=$(date +"%s")
difference=$((currentTime - lastUsedTimestamp))
maxTime=$((2 * 60 * 60)) # 2 hours in seconds

if [ $difference -gt $maxTime ]; then
    /opt/homebrew/bin/blueutil --power 0  # Update the path if blueutil is in a different location
fi
</string>
